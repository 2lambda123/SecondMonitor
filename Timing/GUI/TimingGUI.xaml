<Window x:Name="mainWindow" x:Class="SecondMonitor.Timing.GUI.TimingGUI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SecondMonitor.Timing.GUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Title="Timing" Width="1200" Height="699" Left="1920">
    <Grid>
        <DataGrid x:Name="dtTimig" Margin="0,10,0,0" AutoGenerateColumns="False" CanUserSortColumns="False" CanUserReorderColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding Position}" ClipboardContentBinding="{x:Null}" SortDirection="Ascending" Header="#"/>
                <DataGridTextColumn Binding="{Binding Name}" ClipboardContentBinding="{x:Null}" Header="Name" IsReadOnly="True"/>
            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</Window>
